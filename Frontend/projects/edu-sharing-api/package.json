{
  "name": "edu-sharing-api",
  "version": "0.0.1",
  "scripts": {
    "generate-api:update-openapi-yaml": "cd build && ./generate.sh",
    "generate-api:generate-code": "ng-openapi-gen --input build/openapi.yaml --output src/lib/api",
    "generate-api": "npm run generate-api:update-openapi-yaml && npm run generate-api:generate-code"
  },
  "peerDependencies": {
    "@angular/common": "^12.2.0",
    "@angular/core": "^12.2.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "ng-openapi-gen": "^0.19.0"
  }
}