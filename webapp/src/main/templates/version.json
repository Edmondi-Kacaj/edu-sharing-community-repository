{
  "git": {
    "branch": "${git.branch}",
    "closest": {
      "tag": {
        "name": "${git.closest.tag.fixed}"
      }
    },
    "commit": {
      "id": "${git.commit.id}"
    },
    "dirty": "${git.dirty}"
  },
  "maven": {
    "project": {
      "artifactId": "${project.artifactId}",
      "groupId": "${project.groupId}",
      "version": "${project.version}"
    }
  },
  "version": {
    "main": "${git.closest.tag.fixed}"
  }
}
